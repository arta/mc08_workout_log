=simple_form_for @workout do |f|
  =f.input :date, input_html:{ class:'form-control' }
  =f.input :workout, label: 'What area did you workou?', input_html:{ class:'form-control' }
  =f.input :mood, label: 'How were you feeling?', input_html:{ class:'form-control' }
  =f.input :length, label: 'How long was the workout?', input_html:{ class:'form-control' }
  =f.button :submit
